{
    "name": "bladepack/bladepack",
    "description": "Auto-documentation for your beautiful Blade & Livewire components",
    "license": "MIT",
    "authors": [
        {
            "name": "Joshua Anderton",
            "email": "hey@joshuaanderton.ca"
        }
    ],
    "require": {
        "php": "^8.1|^8.2"
    },
    "autoload": {
        "psr-4": {
            "Bladepack\\Bladepack\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bladepack\\Bladepack\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bladepack\\Bladepack\\Providers\\ServiceProvider"
            ]
        }
    }
  }